group Python;

body( stat ) ::=<<
<stat; separator="\n">
>>

assign( var, expr ) ::=<<
<var> = <expr>;
>>

outArgs( args ) ::=<<
<args; separator=", ">
>>

matrix( el ) ::=<<
matrix( <el> )
>>

call( name, args ) ::=<<
<name><args>
>>

callArgs( args ) ::=<<
(<args; separator=", ">)
>>

horzcat( el ) ::=<<
[<el; separator=", ">]
>>

vertcat( el ) ::=<<
[<el; separator=", ">]
>>

solve( A, b ) ::=<<
linalg.solve( <A>, <b>)
>>
